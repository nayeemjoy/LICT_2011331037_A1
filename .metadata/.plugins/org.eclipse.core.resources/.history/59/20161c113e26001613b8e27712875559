import java.util.Scanner;


public class EvenNumberGenerator {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n;
		
		n = sc.nextInt();
		System.out.println("Even Numbers Are : ");
		for (int i = 0; i <= n; i++) {
			if((i % 2) == 0){
				System.out.println(i);
			}
		}
		sc.close();
	}
}
