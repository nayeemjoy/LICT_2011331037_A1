
public class Bingo {

	public static void main(String[] args) {
		int numbers[] = {7, 25, 5, 19, 30};
		int a,b,sum = 0;
		if(args.length < 2){
			System.out.println("Parameter Missing");
			return ;
		}
		
		
		a = Integer.parseInt(args[0]);
		b = Integer.parseInt(args[1]);
		if(a < 1 || a > 40 || b < 1 || b > 40){
			System.out.print("Number must be Between 1 to 40");
		}
		System.out.print("Your First Namber Was" + a);

		System.out.print("Your Second Namber Was" + b);
		
		

	}

}
